{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "customerName": {
      "type": "string",
      "required": true
    },
    "customerEmail": {
      "type": "email",
      "required": true
    },
    "total": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "orderStatus": {
      "type": "enumeration",
      "required": true,
      "default": "pending",
      "enum": [
        "pending",
        "paid",
        "shipped",
        "delivered",
        "canceled."
      ]
    },
    "trackingCode": {
      "type": "string",
      "unique": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    }
  }
}
